<!-- human_feature_detection.launch -->
<launch>
    <node pkg="human_feature_detection" exec="feature_detect.py" output="screen"/>

    <node pkg="human_feature_detection" exec="feature_detect_3d" output="screen" >
        <param name="topic_name" value="/points2"/>
        <param name="target_frame" value="base_footprint"/>
        <param name="face_range" value="0.20"/>
        <param name="clothes_range" value="0.35"/>
        <param name="bightness_value" value="0"/>
    </node>
</launch>